pipeline {
    agent any
	triggers{PollSCM(" * * * * * ")}
	stages {
        stage('Build') {
            steps {
               	echo 'Building..'
            }
       	}
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}